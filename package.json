{
  "name": "wp-react-plugin",
  "private": true,
  "version": "1.9.4",
  "type": "module",
  "scripts": {
    "bootstrap": "npm install && composer install --no-interaction",
    "dev": "vite",
    "prebuild": "echo 'todo: remove console'",
    "build": "vite build && node release/mv.cjs",
    "preview": "vite preview",
    "format": "prettier-eslint --write \"js/src/**/*.tsx\"",
    "lint": "eslint . --ext .ts,.tsx && phpcbf",
    "lint:fix": "eslint . --ext .ts,.tsx --fix && phpcbf",
    "push": "npm version patch && git push"
  },
  "pre-commit": [
    "lint:fix"
  ],
  "dependencies": {
    "@rollup/plugin-alias": "^5.1.0",
    "@tanstack/react-query": "^5.28.9",
    "axios": "^1.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@kucrut/vite-for-wp": "^0.8.0",
    "@tanstack/react-query-devtools": "^5.28.10",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.2",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@wordpress/eslint-plugin": "^17.11.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^12.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "prettier-eslint-cli": "^8.0.1",
    "prettier-plugin-multiline-arrays": "^3.0.4",
    "sass": "^1.72.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.3",
    "typescript-eslint": "^7.4.0",
    "vite": "^5.2.7",
    "vite-plugin-live-reload": "^3.0.3",
    "vite-tsconfig-paths": "^4.3.2",
    "yarn-upgrade-all": "^0.7.2"
  }
}
